<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <key n="61">
    <name>=</name>
    <desc>Show configuration dialog</desc>
    <binding>
      <command>nasal</command>
      <script>rascal.dialog.toggle()</script>
    </binding>
  </key>

  <key n="23">
    <name>Ctrl-w</name>
    <desc>Place Winch and hook in</desc>
    <binding>
      <command>nasal</command>
      <script>towing.setWinchPositionAuto()</script>
    </binding>
  </key>

  <key n="119">
    <name>w</name>
    <desc>Start winch</desc>
    <binding>
      <command>nasal</command>
      <script>towing.runWinch()</script>
    </binding>
   </key>

  <key n="87">
    <name>W</name>
    <desc>Open winch-hook</desc>
    <binding>
      <command>nasal</command>
      <script>towing.releaseHitch("winch")</script>
    </binding>
  </key>
</PropertyList>

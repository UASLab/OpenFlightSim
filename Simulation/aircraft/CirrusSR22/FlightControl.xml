<?xml version="1.0" ?>
<flight_control name="Generic Flight Control">
    <channel name="Pilot_Inputs">
        <summer name="pilotRoll_norm">
            <input>fcs/aileron-cmd-norm</input>
            <input>fcs/roll-trim-cmd-norm</input>
            <clipto>
                <min>-1.0</min>
                <max>1.0</max>
            </clipto>
            <output>fcs/pilotRoll_norm</output>
        </summer>
        <summer name="pilotPitch_norm">
            <input>fcs/elevator-cmd-norm</input>
            <input>fcs/pitch-trim-cmd-norm</input>
            <clipto>
                <min>-1.0</min>
                <max>1.0</max>
            </clipto>
            <output>fcs/pilotPitch_norm</output>
        </summer>
        <summer name="pilotYaw_norm">
            <input>fcs/rudder-cmd-norm</input>
            <input>fcs/yaw-trim-cmd-norm</input>
            <clipto>
                <min>-1.0</min>
                <max>1.0</max>
            </clipto>
            <output>fcs/pilotYaw_norm</output>
        </summer>
        <summer name="pilotFlap_norm">
            <input>fcs/flap-cmd-norm</input>
            <clipto>
                <min>-1.0</min>
                <max>1.0</max>
            </clipto>
            <output>fcs/pilotFlap_norm</output>
        </summer>
        <pure_gain name="cmdRoll_rps">
            <input>fcs/pilotRoll_norm</input>
            <gain>1.0471975511965976</gain>
            <output>fcs/cmdRoll_rps</output>
        </pure_gain>
        <pure_gain name="cmdPitch_rps">
            <input>fcs/pilotPitch_norm</input>
            <gain>-0.7853981633974483</gain>
            <output>fcs/cmdPitch_rps</output>
        </pure_gain>
        <pure_gain name="cmdYaw_rps">
            <input>fcs/pilotYaw_norm</input>
            <gain>-0.17453292519943295</gain>
            <output>fcs/cmdYaw_rps</output>
        </pure_gain>
        <pure_gain name="cmdFlap_rad">
            <input>fcs/pilotFlap_norm</input>
            <gain>0.3490658503988659</gain>
            <output>fcs/cmdFlap_rad</output>
        </pure_gain>
    </channel>
    <channel name="Control Mixer">
        <pure_gain name="cmdPitch_rps_2_Elevator">
            <input>fcs/cmdPitch_rps</input>
            <gain>-1.6299654065546103e-05</gain>
            <output>fcs/cmdPitch_rps_2_Elevator</output>
        </pure_gain>
        <summer name="cmdElevator_rad">
            <input>fcs/cmdPitch_rps_2_Elevator</input>
            <output>fcs/cmdElevator_rad</output>
        </summer>
        <pure_gain name="cmdYaw_rps_2_Rudder">
            <input>fcs/cmdYaw_rps</input>
            <gain>-7.182011987902733e-05</gain>
            <output>fcs/cmdYaw_rps_2_Rudder</output>
        </pure_gain>
        <summer name="cmdRudder_rad">
            <input>fcs/cmdYaw_rps_2_Rudder</input>
            <output>fcs/cmdRudder_rad</output>
        </summer>
        <pure_gain name="cmdRoll_rps_2_AileronLeft">
            <input>fcs/cmdRoll_rps</input>
            <gain>4.232818756600204e-05</gain>
            <output>fcs/cmdRoll_rps_2_AileronLeft</output>
        </pure_gain>
        <pure_gain name="cmdPitch_rps_2_AileronLeft">
            <input>fcs/cmdPitch_rps</input>
            <gain>-3.258899864050033e-06</gain>
            <output>fcs/cmdPitch_rps_2_AileronLeft</output>
        </pure_gain>
        <pure_gain name="cmdYaw_rps_2_AileronLeft">
            <input>fcs/cmdYaw_rps</input>
            <gain>4.34744241585232e-06</gain>
            <output>fcs/cmdYaw_rps_2_AileronLeft</output>
        </pure_gain>
        <summer name="cmdAileronLeft_rad">
            <input>fcs/cmdRoll_rps_2_AileronLeft</input>
            <input>fcs/cmdPitch_rps_2_AileronLeft</input>
            <input>fcs/cmdYaw_rps_2_AileronLeft</input>
            <output>fcs/cmdAileronLeft_rad</output>
        </summer>
        <pure_gain name="cmdRoll_rps_2_AileronRight">
            <input>fcs/cmdRoll_rps</input>
            <gain>-4.232785405995056e-05</gain>
            <output>fcs/cmdRoll_rps_2_AileronRight</output>
        </pure_gain>
        <pure_gain name="cmdPitch_rps_2_AileronRight">
            <input>fcs/cmdPitch_rps</input>
            <gain>-3.2582060679029173e-06</gain>
            <output>fcs/cmdPitch_rps_2_AileronRight</output>
        </pure_gain>
        <pure_gain name="cmdYaw_rps_2_AileronRight">
            <input>fcs/cmdYaw_rps</input>
            <gain>-4.328357062107747e-06</gain>
            <output>fcs/cmdYaw_rps_2_AileronRight</output>
        </pure_gain>
        <summer name="cmdAileronRight_rad">
            <input>fcs/cmdRoll_rps_2_AileronRight</input>
            <input>fcs/cmdPitch_rps_2_AileronRight</input>
            <input>fcs/cmdYaw_rps_2_AileronRight</input>
            <output>fcs/cmdAileronRight_rad</output>
        </summer>
        <pure_gain name="cmdPitch_rps_2_Flaps">
            <input>fcs/cmdPitch_rps</input>
            <gain>-1.3563985161552912e-05</gain>
            <output>fcs/cmdPitch_rps_2_Flaps</output>
        </pure_gain>
        <summer name="cmdFlaps_rad">
            <input>fcs/cmdPitch_rps_2_Flaps</input>
            <output>fcs/cmdFlaps_rad</output>
        </summer>
    </channel>
    <property>fcs/cmdElevator_ext_rad</property>
    <property>fcs/cmdRudder_ext_rad</property>
    <property>fcs/cmdAileronLeft_ext_rad</property>
    <property>fcs/cmdAileronRight_ext_rad</property>
    <property>fcs/cmdFlaps_ext_rad</property>
    <property>fcs/cmdMotor_ext_nd</property>
    <channel name="External Input Summations">
        <summer>
            <input>fcs/cmdElevator_rad</input>
            <input>fcs/cmdElevator_ext_rad</input>
            <output>fcs/cmdElevator_rad</output>
        </summer>
        <summer>
            <input>fcs/cmdRudder_rad</input>
            <input>fcs/cmdRudder_ext_rad</input>
            <output>fcs/cmdRudder_rad</output>
        </summer>
        <summer>
            <input>fcs/cmdAileronLeft_rad</input>
            <input>fcs/cmdAileronLeft_ext_rad</input>
            <output>fcs/cmdAileronLeft_rad</output>
        </summer>
        <summer>
            <input>fcs/cmdAileronRight_rad</input>
            <input>fcs/cmdAileronRight_ext_rad</input>
            <output>fcs/cmdAileronRight_rad</output>
        </summer>
        <summer>
            <input>fcs/cmdFlaps_rad</input>
            <input>fcs/cmdFlaps_ext_rad</input>
            <output>fcs/cmdFlaps_rad</output>
        </summer>
        <summer>
            <input>fcs/throttle-cmd-norm</input>
            <input>fcs/cmdMotor_ext_nd</input>
            <output>fcs/throttle-pos-norm</output>
        </summer>
    </channel>
</flight_control>

<?xml version="1.0" encoding="utf-8"?>
<output type="CSV" name="jsbsim_log.csv" rate="50">
    <simulation> ON </simulation>
    <atmosphere> ON </atmosphere>
    <massprops> ON </massprops>
    <rates> ON </rates>
    <velocities> ON </velocities>
    <forces> OFF </forces>
    <moments> OFF </moments>
    <position> ON </position>
    <propulsion> OFF </propulsion>
    <aerosurfaces> OFF </aerosurfaces>
    <fcs> OFF </fcs>
    <ground_reactions> OFF </ground_reactions>
    <coefficients> OFF </coefficients>

    <property> sensor/imu/time_us </property>
    <property> sensor/imu/accelX_true_fps2 </property>
    <property> sensor/imu/accelY_true_fps2 </property>
    <property> sensor/imu/accelZ_true_fps2 </property>

    <property> sensor/gps/time_us </property>
    <property> sensor/gps/lat_true_rad </property>
    <property> sensor/gps/long_true_rad </property>
    <property> sensor/gps/alt_true_m </property>
    <property> sensor/gps/vNorth_true_mps </property>
    <property> sensor/gps/vEast_true_mps </property>
    <property> sensor/gps/vDown_true_mps </property>

    <property> sensor/pitot/time_us </property>
    <property> sensor/pitot/presStatic_true_Pa </property>
    <property> sensor/pitot/presTip_true_Pa </property>
    <property> sensor/pitot/temp_true_C </property>

    <property> sensor/fiveHole/presStatic_true_Pa </property>
    <property> sensor/fiveHole/presTip_true_Pa </property>
    <property> sensor/fiveHole/presAlpha_true_Pa </property>
    <property> sensor/fiveHole/presAlphaBot_true_Pa </property>
    <property> sensor/fiveHole/presAlphaTop_true_Pa </property>
    <property> sensor/fiveHole/presBeta_true_Pa </property>
    <property> sensor/fiveHole/presBetaRight_true_Pa </property>
    <property> sensor/fiveHole/presBetaLeft_true_Pa </property>
    <property> sensor/fiveHole/temp_true_C </property>

    <property> sensor/cmd/time_us </property>

</output>
